{
  <% sex=gender %>
  "first": "<%= forename(sex) %>",
  "last": "<%= surname %>",
  "gender": "<%= sex %>",
  "birthdate": <%= between(Time.gm(1940, 1, 1), Time.gm(1990, 12, 31)) %>,
  "measures": {
    "0041": {
      "dummy": "entry"
      ,"encounter_prev_med_40_or_older": <%= n_between(Time.gm(2010, 1, 1), Time.gm(2010, 12, 31), 0, 4) %>
      ,"immunization": <%= n_between(Time.gm(2010, 9, 1), Time.gm(2010, 12, 31), 0, 1) %>
      <% if percent(50) %>
      ,"allergy_to_eggs": true
      <% end %>
    }
  }
}